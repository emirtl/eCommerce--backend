{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "consistent-return": "off",
        "no-unused-vars": "off",
        "no-underscore-dangle": "off",
        "prefer-destructuring": "off",
        "array-callback-return": "off",
        "operator-assignment": "off"
    }
}
